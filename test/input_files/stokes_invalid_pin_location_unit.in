# Mesh related options
[Mesh]
   [./Generation]
      dimension = '2'
      element_type = 'QUAD9'
      n_elems_x = '10'
      n_elems_y = '10'
      x_max = '5.0'
[]

# Options for time solvers



# Options for print info to the screen
[screen-options]
print_equation_system_info = 'true'
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

echo_physics = 'true'

[Materials]
   [./TestMaterial]
      [./Viscosity]
        model = 'constant'
        value = '1.0'
    [../Density]
      value = '1.0'
[]

# Options related to all Physics
[Physics]

enabled_physics = 'Stokes'

# Options for Stokes physics
[./Stokes]

material = 'TestMaterial'

bc_ids = '1 1 3 3 2 0'
bc_types = 'parsed_dirichlet parsed_dirichlet  parsed_dirichlet  parsed_dirichlet   no_slip no_slip'
bc_variables = 'u v u v na na'
bc_values = '4*y*(1-y) 0.0 4*y*(1-y) 0.0 na na'

pin_pressure = 'true'
pin_value = '100.0'

# Purposefully outside the mesh for this test
pin_location = '-2.5 0.5'

[]

[Variables]
   [./Velocity]
      names = 'u v'
      fe_family = 'LAGRANGE'
      order = 'SECOND'
   [../Pressure]
      names = 'p'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]
